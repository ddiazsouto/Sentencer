pipeline {
    agent any
    stages{

        stage('Stage 1: Build/search'){
            steps{
                sh "whoami"
            }
        }

        stage('Stage 2: Test/show'){
            steps{
                sh "sudo ls"
            }
        }

        stage('Stage 3: Deploy/do'){
            steps{
                sh "pwd "
            }
        }
    }
}